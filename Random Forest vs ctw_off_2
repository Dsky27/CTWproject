   "source": [
    "from tqdm import tqdm\n",
    "\n",
    "# === 1. Fonction d'encodage personnalis√©e ===\n",
    "def convert_to_classes(r):\n",
    "    if r < -0.005:\n",
    "        return 0  # Strong Negative\n",
    "    elif -0.005 <= r < 0:\n",
    "        return 1  # Slightly Negative\n",
    "    elif 0 <= r < 0.005:\n",
    "        return 2  # Slightly Positive\n",
    "    else:\n",
    "        return 3  # Strong Positive"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
